{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "^4",
        "silverstripe-themes/simple": "~3.2.0",
        "dnadesign/silverstripe-elemental": "3.0.x-dev",
        "dnadesign/silverstripe-elemental-list": "^1.0",
        "silverstripe/blog": "^3.2",
        "heyday/silverstripe-menumanager": "^3.0",
        "tractorcow/silverstripe-opengraph": "^4",
        "unclecheese/display-logic": "^2.0",
        "bummzack/sortablefile": "^2.1",
        "nathancox/codeeditorfield": "v2.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ],
        "expose": [
            "themes/GTM/assets/js",
            "themes/GTM/assets/css",
            "themes/GTM/assets/img",
            "themes/GTM/assets/vendor",
            "themes/GTM/fonts",
            "themes/GTM/src"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
